{
    "name": "homesale",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "npm run db:prod && next build",
        "start": "next start",
        "lint": "next lint",
        "add": "git status && git add . && git status",
        "commit": "npm run add && git commit -m 'fast commit' && git push",
        "update": "ncu -u && npm i && npm run add && git commit -m 'Updated modules'",
        "db:push": "npx prisma db push",
        "db:dev": "npx prisma migrate dev",
        "db:prod": "npx prisma generate && npx prisma migrate deploy"
    },
    "dependencies": {
        "react": "^18",
        "react-dom": "^18",
