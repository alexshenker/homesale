        "@mui/material": "^5.15.15",
        "@next-auth/prisma-adapter": "^1.0.7",
        "@prisma/client": "^5.12.1",
        "@tanstack/react-query": "^5.29.2",
        "@tanstack/react-query-devtools": "^5.29.2",
        "autoprefixer": "10.4.19",
        "aws-sdk": "^2.1600.0",
        "clsx": "^2.1.0",
        "date-fns": "^3.6.0",
